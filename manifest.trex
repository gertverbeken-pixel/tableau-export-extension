<?xml version="1.0" encoding="utf-8"?>
<manifest manifest-version="0.1" xmlns="http://www.tableau.com/xml/extension_manifest">
  <dashboard-extension id="com.easyfairs.exportall.minimal" extension-version="1.0.0">
    <default-locale>en_US</default-locale>
    <name resource-id="name"/>
    <description>Minimal Export All Extension</description>
    <author name="Gert Verbeken" email="gert.verbeken@easyfairs.com" organization="Easyfairs" website="https://gertverbeken-pixel.github.io"/>
    <min-api-version>1.0</min-api-version>
    <source-location>
      <url>https://gertverbeken-pixel.github.io/tableau-export-extension/</url>
    </source-location>
    <icon></icon>
  </dashboard-extension>

  <!-- âœ… Correctly placed permissions -->
  <permissions>
    <scope>UnderlyingData</scope>
  </permissions>

  <resources>
    <resource id="name">
      <text locale="en_US">Export All Minimal</text>
    </resource>
  </resources>
</manifest>